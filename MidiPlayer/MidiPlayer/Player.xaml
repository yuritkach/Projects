      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MidiPlayer"
      mc:Ignorable="d" 
      d:DesignHeight="667.208" d:DesignWidth="1386.441"
      Title="Player">

    <Grid RenderTransformOrigin="0.498,0.431">
        <Grid.RowDefinitions>
            <RowDefinition Height="39"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Canvas x:Name="StuffCanvas" Grid.Row="1"/>
