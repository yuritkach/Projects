            <Button x:Name="button" Content="Button" Margin="2"/>
            <Button x:Name="button1" Content="Button" Margin="2"/>
            <Button x:Name="button2" Content="Button" Margin="2"/>
            <Button x:Name="button3" Content="Button" Margin="2"/>
        </StackPanel>
